<div class="event new form">

  <h1>New Event</h1>

  <%= form_for @event do |f| %>
    <div class="title">
      <%= f.label :title %>
      <%= f.text_field :title %>
    </div>
    <div class="parents">
      <%= f.label :parent_tokens, "Parents" %>
      <%= f.text_field :parent_tokens %>
    </div>
    <div class="children">
      <%= f.label :child_tokens, "Children" %>
      <%= f.text_field :child_tokens %>
    </div>
    <%= f.submit %>
  <% end %>

</div>
