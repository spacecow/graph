<% present remark do |presenter| %>

<li class="remark">
  <span class="content"><%= presenter.content %></span>
  <span class="actions">
    <span class="edit"><%= presenter.edit_link event_id:event_id %></span>
    <span class="delete"><%= presenter.delete_link event_id:event_id %></span>
  </span>
</li>

<% end %>
